{
  "name": "@momsfriendlydevco/vitel",
  "version": "1.1.30",
  "description": "Various, pickable Vue@3 / Vite modules and components",
  "scripts": {
    "build-storybook": "storybook build",
    "dev": "storybook dev --port=32000 --no-open",
    "lint": "eslint ."
  },
  "keywords": [
    "vite",
    "components"
  ],
  "type": "module",
  "imports": {
    "#components/*": "./components/*.vue",
    "#directives/*": "./directives/*.vue",
    "#filters/*": "./filters/*.js",
    "#services/*": "./services/*.vue",
    "#vitel": "./lib/vitel.js"
  },
  "exports": {
    ".": "./lib/vitel.js",
    "./components/*": "./components/*.vue",
    "./directives/*": "./directives/*.vue",
    "./filters/*": "./filters/*.js",
    "./service": "./lib/service.js",
    "./services/*": "./services/*.vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MomsFriendlyDevCo/Vitel.git"
  },
  "author": "Matt Carter <matt@mfdc.biz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MomsFriendlyDevCo/Vitel/issues"
  },
  "homepage": "https://github.com/MomsFriendlyDevCo/Vitel#readme",
  "engineStrict": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.2.0",
    "@kinde-oss/kinde-auth-pkce-js": "^3.0.22",
    "@momsfriendlydevco/formatters": "^1.2.3",
    "@supabase/supabase-js": "^2.38.1",
    "@teamhanko/hanko-elements": "^0.8.2",
    "@zip.js/zip.js": "^2.7.30",
    "axios": "^1.5.1",
    "json-markup": "^1.1.4",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "object-hash": "^3.0.0",
    "vue3-toastify": "^0.1.12"
  },
  "devDependencies": {
    "@momsfriendlydevco/eslint-config": "^1.0.7",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.2.3",
    "@storybook/vue3-vite": "^7.2.3",
    "bootstrap": "^5.3.1",
    "eslint": "^8.48.0",
    "msw": "^1.3.0",
    "msw-storybook-addon": "^1.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.65.1",
    "storybook": "^7.2.3"
  },
  "eslintConfig": {
    "extends": "@momsfriendlydevco",
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
