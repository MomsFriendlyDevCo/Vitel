{
  "name": "@momsfriendlydevco/vitel",
  "version": "1.1.1",
  "description": "Various, pickable Vue@3 / Vite modules and components",
  "scripts": {
    "build-storybook": "storybook build",
    "dev": "storybook dev --port=32000 --no-open",
    "lint": "eslint ."
  },
  "keywords": [
    "vite",
    "components"
  ],
  "type": "module",
  "exports": {
    ".": "./lib/vitel.js",
    "./components/*": "./components/*.vue",
    "./directives/*": "./directives/*.vue",
    "./filters/*": "./filters/*.js",
    "./services/*": "./services/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MomsFriendlyDevCo/Vitel.git"
  },
  "author": "Matt Carter <matt@mfdc.biz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MomsFriendlyDevCo/Vitel/issues"
  },
  "homepage": "https://github.com/MomsFriendlyDevCo/Vitel#readme",
  "engineStrict": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "devDependencies": {
    "@momsfriendlydevco/eslint-config": "^1.0.5",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.2.3",
    "@storybook/vue3-vite": "^7.2.3",
    "bootstrap": "^5.3.1",
    "eslint": "^8.47.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.65.1",
    "storybook": "^7.2.3"
  },
  "eslintConfig": {
    "extends": "@momsfriendlydevco",
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
    }
  },
  "dependencies": {
    "vue3-toastify": "^0.1.12"
  }
}
