{
  "name": "@momsfriendlydevco/vitel",
  "version": "1.0.0",
  "description": "Various, pickable Vue / Vite modules and components",
  "scripts": {
    "build": "storybook build --output-dir=docs",
    "dev": "storybook dev -p 6006",
    "lint": "eslint ."
  },
  "keywords": [
    "vite",
    "components"
  ],
  "type": "module",
  "exports": {
    ".": "./lib/vitel.js",
    "./components/*": "./components/*.js",
    "./services/*": "./services/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MomsFriendlyDevCo/Vitel.git"
  },
  "author": "Matt Carter <matt@mfdc.biz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MomsFriendlyDevCo/Vitel/issues"
  },
  "homepage": "https://github.com/MomsFriendlyDevCo/Vitel#readme",
  "engineStrict": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "@momsfriendlydevco/eslint-config": "^1.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.2.2",
    "@storybook/addon-essentials": "^7.2.2",
    "@storybook/addon-interactions": "^7.2.2",
    "@storybook/addon-links": "^7.2.2",
    "@storybook/blocks": "^7.2.2",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.2.2",
    "@storybook/vue3-vite": "^7.2.2"
  },
  "eslintConfig": {
    "extends": "@momsfriendlydevco",
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
    }
  }
}
